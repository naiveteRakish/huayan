<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="whayer.cloud.dbSystemInfoDao">
 <class name="DbSystemInfo" table="dbsysteminfo">
  	<id column="id" name="id" type="long">
   		<generator class="assigned"/>
  	</id>
  	<property column="name" generated="never" lazy="false" length="500" name="name" type="string"/>
  	<property column="version" generated="never" lazy="false" length="500" name="version" type="string"/>
 </class>
</hibernate-mapping>



































