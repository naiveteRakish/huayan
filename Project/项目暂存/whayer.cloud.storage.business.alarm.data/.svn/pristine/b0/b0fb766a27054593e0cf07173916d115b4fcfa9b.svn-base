package com.whayer.cloud.storage.business.alarm.data;

import com.whayer.cloud.storage.business.base.data.Business;
import com.whayer.cloud.storage.business.base.data.msg.MessageBody;
import com.whayer.cloud.storage.business.base.data.msg.MessageGenFeature;
import com.whayer.cloud.storage.business.base.data.msg.MessageLevel;
import com.whayer.cloud.storage.business.base.data.msg.MessageSource;

/**
 * @author fs 告警信息对象
 * 
 *         告警信息对象是告警查询与存取的业务数据对象
 */
public class AlarmInfo extends Business {
	/**
	 * 消息源
	 */
	private MessageSource sourceObject;

	/**
	 * 告警类型
	 */
	private AlarmMessageTypeMeta alarmMessageTypeMeta;

	/**
	 * 告警级别
	 */
	private MessageLevel msgLevel;

	/**
	 * 消息产生特性
	 */
	private MessageGenFeature msgGenerateType;

	/**
	 * 告警开始时间
	 */
	private String alarmStartTime;

	/**
	 * 告警结束时间
	 */
	private String alarmEndTime;

	/**
	 * 告警附带值
	 */
	private MessageBody alarmBody;

	/**
	 * 获取
	 * 
	 * @return sourceObject
	 */
	public MessageSource getSourceObject() {
		return sourceObject;
	}

	/**
	 * 设置
	 * 
	 * @param sourceObject
	 */
	public void setSourceObject(MessageSource sourceObject) {
		this.sourceObject = sourceObject;
	}

	/**
	 * 获取
	 * 
	 * @return alarmMessageTypeMeta
	 */
	public AlarmMessageTypeMeta getAlarmMessageTypeMeta() {
		return alarmMessageTypeMeta;
	}

	/**
	 * 设置
	 * 
	 * @param alarmMessageTypeMeta
	 */
	public void setAlarmMessageTypeMeta(AlarmMessageTypeMeta alarmMessageTypeMeta) {
		this.alarmMessageTypeMeta = alarmMessageTypeMeta;
	}

	/**
	 * 获取
	 * 
	 * @return msgLevel
	 */
	public MessageLevel getMsgLevel() {
		return msgLevel;
	}

	/**
	 * 设置
	 * 
	 * @param msgLevel
	 */
	public void setMsgLevel(MessageLevel msgLevel) {
		this.msgLevel = msgLevel;
	}

	/**
	 * 获取
	 * 
	 * @return msgGenerateType
	 */
	public MessageGenFeature getMsgGenerateType() {
		return msgGenerateType;
	}

	/**
	 * 设置
	 * 
	 * @param msgGenerateType
	 */
	public void setMsgGenerateType(MessageGenFeature msgGenerateType) {
		this.msgGenerateType = msgGenerateType;
	}

	/**
	 * 获取
	 * 
	 * @return alarmStartTime
	 */
	public String getAlarmStartTime() {
		return alarmStartTime;
	}

	/**
	 * 设置
	 * 
	 * @param alarmStartTime
	 */
	public void setAlarmStartTime(String alarmStartTime) {
		this.alarmStartTime = alarmStartTime;
	}

	/**
	 * 获取
	 * 
	 * @return alarmEndTime
	 */
	public String getAlarmEndTime() {
		return alarmEndTime;
	}

	/**
	 * 设置
	 * 
	 * @param alarmEndTime
	 */
	public void setAlarmEndTime(String alarmEndTime) {
		this.alarmEndTime = alarmEndTime;
	}

	/**
	 * 获取
	 * 
	 * @return alarmBody
	 */
	public MessageBody getAlarmBody() {
		return alarmBody;
	}

	/**
	 * 设置
	 * 
	 * @param alarmBody
	 */
	public void setAlarmBody(MessageBody alarmBody) {
		this.alarmBody = alarmBody;
	}

}
