<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:motan="http://api.weibo.com/schema/motan"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">

	<motan:protocol id="motan-protocol" default="true" name="motan"
		serialization="fastjson" maxServerConnection="80000" maxContentLength="1048576"
		maxWorkerThread="800" minWorkerThread="20" />

	<motan:basicReferer id="motanClientBasicConfig"
		requestTimeout="2000" accessLog="false" retries="2" group="motan-demo-provider"
		module="motan-demo-provider" application="motan-demo-provider"
		protocol="motan-protocol" throwException="false" check="true" />

	<motan:referer id="resourceService"
		interface="com.whayer.cloud.storage.component.foundation.res.IResourceService"
		directUrl="192.168.40.80:8002" basicReferer="motanClientBasicConfig" />
</beans>
